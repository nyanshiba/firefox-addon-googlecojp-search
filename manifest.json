{
    "manifest_version": 2,
    "name": "www.google.co.jp",
    "description": "デスクトップ版Firefoxの検索エンジンに、 www.google.com よりRTTの良い www.google.co.jp を追加する最小限のアドオン",
    "version": "1.0.0",
    "author": "nyanshiba",
    "homepage_url": "https://github.com/nyanshiba/firefox-addon-googlecojp-search",
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "www.google.co.jp",
            "search_url": "https://www.google.co.jp/search?q={searchTerms}",
            "is_default": false,
            "favicon_url": "https://www.google.co.jp/favicon.ico",
            "keyword": "jp"
        }
    }
}
